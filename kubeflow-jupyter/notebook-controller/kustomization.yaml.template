apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
namespace: ${kubernetes.namespace}
commonLabels:
  app.kubernetes.io/component: notebook-controller

resources:
- upstream/overlays/kubeflow

configMapGenerator:
- name: config
  behavior: merge
  literals:
  - ISTIO_GATEWAY=${kubernetes.namespace}/kubeflow-gateway
