kind: RoleBinding
metadata:
  name: default-editor
  namespace: ${component.kubeflow.namespace}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: kubeflow-edit
subjects:
- kind: ServiceAccount
  name: default-editor
  namespace: ${component.kubeflow.namespace}
