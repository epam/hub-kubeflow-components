{{- if .azure.storageAccount.name }}
artifactRepository:
  azure:
    endpoint: "https://{{.azure.storageAccount.name}}.blob.core.windows.net"
    container: "{{.azure.storageAccount.containerName}}"
    useSDKCreds: false
    accountKeySecret:
      name: "{{.hub.componentName}}-artifacts"
      key: accesskey
{{- end }}
